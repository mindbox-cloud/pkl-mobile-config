import "../_SettingsBase.pkl" as base

local ops = base.settings.operations
local newOps = new Dynamic {
  viewProduct = ops.viewProduct
  viewCategory = new Dynamic { systemNameTest = ops.viewCategory.systemName }
  setCart = new Dynamic { systemName = 123 }
}

output {
  renderer = new JsonRenderer {}
  value = base.settings.toMap()
    .mapValues((key, value) -> if (key == "operations") newOps else value)
    .toDynamic()
}
