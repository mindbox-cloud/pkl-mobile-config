import "_SettingsInAppBase.pkl" as base

local brokenInapp = new Dynamic {
  maxInappsPerSession = 1
  maxInappsPerDay = "invalid"
  minIntervalBetweenShowsTest = "0.00:00:10"
}

output {
  renderer = new JsonRenderer {}
  value = base.settings.toMap()
    .mapValues((key, value) -> if (key == "inapp") brokenInapp else value)
    .toDynamic()
}
