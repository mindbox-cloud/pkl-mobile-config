import "../_SettingsBase.pkl" as base

output {
  renderer = new JsonRenderer {}
  value = base.settings.toMap()
    .mapValues((key, value) ->
      if (key == "ttl") new Dynamic { inapps = "2.00:00:00" }
      else if (key == "slidingExpiration") new Dynamic { config = "2.00:00:00"; pushTokenKeepaliveTest = "14.00:00:00" }
      else value
    )
    .toDynamic()
}
