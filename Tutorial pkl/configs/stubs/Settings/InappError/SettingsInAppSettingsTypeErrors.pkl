import "../_SettingsInAppBase.pkl" as base

output {
  renderer = new JsonRenderer {}
  value = base.settings.toMap()
    .mapValues((key, value) ->
      if (key == "inapp") value.toMap()
        .mapValues((k, v) ->
          if (k == "maxInappsPerSession") "invalid"
          else if (k == "maxInappsPerDay") "invalid"
          else if (k == "minIntervalBetweenShows") 123
          else v
        )
        .toDynamic()
      else value
    )
    .toDynamic()
}
