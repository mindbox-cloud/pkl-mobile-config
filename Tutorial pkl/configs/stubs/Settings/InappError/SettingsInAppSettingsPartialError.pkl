import "../_SettingsInAppBase.pkl" as base

output {
  renderer = new JsonRenderer {}
  value = base.settings.toMap()
    .mapValues((key, value) ->
      if (key == "inapp") value.toMap()
        .mapValues((k, v) -> if (k == "maxInappsPerDay") "invalid" else v)
        .mapKeys((k, _) -> if (k == "minIntervalBetweenShows") "minIntervalBetweenShowsTest" else k)
        .toDynamic()
      else value
    )
    .toDynamic()
}
