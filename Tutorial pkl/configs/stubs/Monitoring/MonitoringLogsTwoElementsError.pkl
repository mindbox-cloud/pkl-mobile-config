import "../../../templates/MonitoringTemplate.pkl"

logs = new Listing {
  MonitoringTemplate.logs[0].toMap()
    .mapKeys((key, _) -> if (key == "requestId") "request" else key)
    .toDynamic()
  MonitoringTemplate.logs[1].toMap()
    .mapKeys((key, _) -> if (key == "deviceUUID") "device" else key)
    .toDynamic()
}
